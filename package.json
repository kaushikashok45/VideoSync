{
  "name": "videosync",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "setuphttps": "openssl req -x509 -newkey rsa:2048 -nodes -keyout localhost.key -out localhost.pem -days 365 -config localhost.conf -extensions v3_req",
    "build": "react-router build",
    "development": "react-router dev",
    "dev": "node server.js --NODE_ENV=dev",
    "prod": "yarn run build;node server.js --NODE_ENV=production",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-router/express": "^7.13.0",
    "@react-router/fs-routes": "^7.13.0",
    "@react-router/node": "^7.13.0",
    "@types/express": "^5.0.3",
    "@types/simple-peer": "^9.11.8",
    "cors": "^2.8.5",
    "express": "4.20.0",
    "isbot": "^4.1.0",
    "livekit-client": "^2.13.4",
    "nanoid": "^5.1.6",
    "npm": "^11.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.0.0",
    "react-router-dom": "^7.13.0",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.6"
  },
  "devDependencies": {
    "@react-router/dev": "^7.13.0",
    "@types/cors": "^2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "autoprefixer": "^10.4.19",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^6.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@4.12.0"
}
